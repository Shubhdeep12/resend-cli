{
  "name": "@shubhdeep12/resend-cli",
  "version": "0.3.0",
  "type": "module",
  "description": "CLI for Resend",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "access": "public",
  "bin": {
    "resend-cli": "dist/index.cjs",
    "resend": "dist/index.cjs"
  },
  "files": ["dist/**"],
  "engines": {
    "node": ">=20"
  },
  "keywords": ["resend", "email", "cli", "command-line"],
  "author": "Shubhdeep (@shubhdeep12)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shubhdeep12/resend-cli.git"
  },
  "bugs": {
    "url": "https://github.com/shubhdeep12/resend-cli/issues"
  },
  "homepage": "https://github.com/shubhdeep12/resend-cli#readme",
  "scripts": {
    "build": "tsdown src/index.ts --format esm,cjs --dts",
    "dev": "tsdown src/index.ts --format esm,cjs --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version-packages": "pnpm changeset version && pnpm install --no-frozen-lockfile",
    "release": "pnpm run version-packages && git add -A && (git diff --staged --quiet || (git commit -m \"chore: version packages\" && git push)) && pnpm publish --access public"
  },
  "dependencies": {
    "@clack/prompts": "0.2.2",
    "@stricli/auto-complete": "^1.2.5",
    "@stricli/core": "^1.2.5",
    "cli-table3": "0.6.5",
    "conf": "13.1.0",
    "dotenv": "16.4.7",
    "picocolors": "1.1.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "resend": "^6.9.2",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "^2.29.8",
    "@types/node": "22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "tsdown": "0.20.1",
    "typescript": "5.7.3",
    "vitest": "3.0.5",
    "vitest-fetch-mock": "0.4.5"
  }
}
